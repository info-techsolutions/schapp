<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Set Report Title</ReportTitle><ReportDescription></ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>select cac.position as pos, aa.label acct_name,
ROUND(SUM(IF(-1 = -1, bk.montant,0)),2) SelectedPeriod,
ROUND(SUM(IF(MONTH(bk.doc_date)=1,bk.montant,0)),2) Jan,
ROUND(SUM(IF(MONTH(bk.doc_date)=2,bk.montant,0)),2) Feb,
ROUND(SUM(IF(MONTH(bk.doc_date)=3,bk.montant,0)),2) Mar,
ROUND(SUM(IF(MONTH(bk.doc_date)=4,bk.montant,0)),2) Apr,
ROUND(SUM(IF(MONTH(bk.doc_date)=5,bk.montant,0)),2) May,
ROUND(SUM(IF(MONTH(bk.doc_date)=6,bk.montant,0)),2) Jun,
ROUND(SUM(IF(MONTH(bk.doc_date)=7,bk.montant,0)),2) Jul,
ROUND(SUM(IF(MONTH(bk.doc_date)=8,bk.montant,0)),2) Aug,
ROUND(SUM(IF(MONTH(bk.doc_date)=9,bk.montant,0)),2) Sep,
ROUND(SUM(IF(MONTH(bk.doc_date)=10,bk.montant,0)),2) Oct,
ROUND(SUM(IF(MONTH(bk.doc_date)=11,bk.montant,0)),2) Nov,
ROUND(SUM(IF(MONTH(bk.doc_date)=12,bk.montant,0)),2) Dece,
ROUND(SUM(bk.montant),2) as Total,
ROUND(SUM(IF(cac.position=10,bk.montant,0)),2) - ROUND(SUM(IF(cac.position=20,bk.montant,0)),2) as GROSS,
ROUND(SUM(IF(cac.position=10,bk.montant,0)),2) - ROUND(SUM(IF(cac.position=20,bk.montant,0)),2)- ROUND(SUM(IF(cac.position=40,bk.montant,0)),2) as NET

FROM llx_c_accounting_category cac
left join llx_accounting_account aa on  cac.rowid = aa.fk_accounting_category
left join (select * from llx_accounting_bookkeeping as bkk where bkk.doc_date between {daterange,RANGE1} and {daterange,RANGE2} ) as bk on aa.account_number = bk.numero_compte
GROUP BY acct_name, pos ORDER BY pos</SQLRaw></SQL><QueryColumns><QueryColumn><Name>pos</Name><TableName></TableName><ColumnName>cac.position as</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>acct_name</Name><TableName>aa</TableName><ColumnName>label</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>SelectedPeriod</Name><TableName></TableName><ColumnName>ROUND(SUM(IF(-1 = -1, bk.montant,0)),2)</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>Jan</Name><TableName></TableName><ColumnName>RO